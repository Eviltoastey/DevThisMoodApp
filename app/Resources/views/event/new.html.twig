{% extends 'base.html.twig' %}

{% block body %}
    <div class="full">
        <div class="col-sm-2"></div>
        <div class="col-sm-8 event-new">
            <h1>Nieuw agenda punt</h1>
            {{ form_start(form) }}
            <label>Start datum</label>
            {{ form_widget(form.startDate, { 'attr': {'class': 'js-datepicker'}, 'label': 'Start datum'})  }}
            <label>Eind datum'</label>
            {{ form_widget(form.endDate, { 'attr': {'class': ' js-datepicker'}, 'label': 'eind datum'})  }}
            <label>Omschrijving</label>
            {{ form_widget(form.description, { 'attr': {'class': 'form-control'}, 'label': 'omschrijving'})  }}
            <input type="submit" value="Create" />
            {{ form_end(form) }}
        </div>
        <div class="col-sm-2"></div>
    </div>
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.6/jquery.min.js" type="text/javascript"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8/jquery-ui.min.js" type="text/javascript"></script>
    <link href="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8/themes/base/jquery-ui.css" rel="Stylesheet" type="text/css" />
{% endblock %}
